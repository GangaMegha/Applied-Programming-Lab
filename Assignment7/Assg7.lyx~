\begin_layout Plain Layout

t=np.linspace(0,0.0001,num=10005)
\end_layout

\begin_layout Plain Layout

vi=np.cos(1000*t)-np.cos(1000000*t)
\end_layout

\begin_layout Plain Layout

t,y,svec=sp.lsim(H,vi,t)
\end_layout

\begin_layout Plain Layout

mp.figure(3)
\end_layout

\begin_layout Plain Layout

mp.plot(t,y)
\end_layout

\begin_layout Plain Layout

mp.title(r'Plot of $v_{0}$(t)=$v_{i}$(t) * h(t)')
\end_layout

\begin_layout Plain Layout

mp.xlabel("t")
\end_layout

\begin_layout Plain Layout

mp.ylabel(r'$v_{0}$(t)')
\end_layout

